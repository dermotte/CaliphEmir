      <Properties>
        <Property name="editable" type="boolean" value="false"/>
        <Property name="toolTipText" type="java.lang.String" value="c1 is multiplied with the logarithmic attraction force: force = c1 * Math.log(nodeA.distance(nodeB) / c2)"/>
