    </Component>
    <Component class="javax.swing.JTextField" name="c2">
      <Properties>
        <Property name="editable" type="boolean" value="false"/>
        <Property name="toolTipText" type="java.lang.String" value="c2 defines how much nodes attract each other: divisor of the distance: force = c1 * Math.log(nodeA.distance(nodeB) / c2)"/>
      </Properties>
    </Component>
    <Component class="javax.swing.JTextField" name="c3">
      <Properties>
        <Property name="editable" type="boolean" value="false"/>
        <Property name="toolTipText" type="java.lang.String" value="c3 defines how much nodes repel each other: force = -c3 / Math.sqrt(nodeA.distance(nodeB));"/>
      </Properties>
    </Component>
